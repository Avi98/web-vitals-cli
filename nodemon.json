{
  "restartable": "rs",
  "ignore": [".git", "node_modules/**/node_modules"],
  "verbose": true,
  "execMap": {
    "ts": "node --require ts-node/register"
  },
  "watch": ["src/"],
  "exec":"node --inspect-brk ts-node src/index.ts",
  "env": {
    "NODE_ENV": "develop"
  },
  "ext": "js,json,ts"
}
